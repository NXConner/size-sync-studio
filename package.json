    "e2e": "playwright test",
    "e2e:smoke": "playwright test -g \"Measure smoke\"",
    "cap:sync": "npx cap sync",
    "cap:ios": "npx cap add ios && npx cap sync ios",
    "cap:ios:open": "npx cap open ios",
