    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run --coverage --coverage.enabled true --coverage.lines 0.8 --coverage.functions 0.8 --coverage.branches 0.8 --coverage.statements 0.8",
